{
  "name": "x-profile-name-tag-generator",
  "version": "1.0.0",
  "description": "Generate printable name tags from X (Twitter) profile information",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "x",
    "twitter",
    "name-tag",
    "pdf",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pdfkit": "^0.14.0",
    "puppeteer": "^21.5.2",
    "qrcode": "^1.5.3",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@types/pdfkit": "^0.12.12",
    "@types/qrcode": "^1.5.5",
    "copy-webpack-plugin": "^13.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  }
}
